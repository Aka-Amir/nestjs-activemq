{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@medad-techs/nestjs-active-mq/active-mq/(.*)": "<rootDir>/../libs/active-mq/src/$1",
    "@medad-techs/nestjs-active-mq/active-mq": "<rootDir>/../libs/active-mq/src"
  }
}